package br.com.sistemapizzaria.model;

import br.com.sistemapizzaria.dao.LoginDao;

public class Cliente extends LoginDao {

	private String nome;

	@Override
	public void setUser(String user) {

		super.setUser(user);
	}

	@Override
	public void setPassword(String password) {

		super.setPassword(password);
	}

	public String getNome() {
		return nome;
	}

//	public void setNome(String nome) {
//		this.nome = nome;
//	}

	public Cliente(String nome, String username, String password) {
		this.nome = nome;
		if(super.getUser() == username && super.getPassword() == password) {
			System.out.println(getNome() + "Logou");
		}
		
	}

	@Override
	public String toString() {
		return "Cliente [nome=" + nome + "]";
	}
	

}
